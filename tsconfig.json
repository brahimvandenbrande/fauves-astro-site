{
  "extends": "astro/tsconfigs/strict",
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.astro",
    "**/*.d.ts",
    ".astro/types.d.ts"
  ],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@public/*": ["public/*"],
      "@components/*": ["src/components/*"],
      "@content/*": ["src/content/*"],
      "@helpers/*": ["src/helpers/*"],
      "@layouts/*": ["src/layouts/*"],
      "@pages/*": ["src/pages/*"],
      "@styles/*": ["src/styles/*"],
      "@consts": ["src/consts.ts"],
      "@assets/*": ["./src/assets/*"],
      "@/*": ["./src/*"]
    },
    "types": ["astro/client"]
  }
}
